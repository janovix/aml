	"scripts": {
		"build": "next build",
		"cf-build": "opennextjs-cloudflare build",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
		"prepare": "husky",
		"typecheck": "tsc --noEmit",
		"check": "pnpm run build && pnpm run typecheck",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"dev": "next dev --webpack",
		"dev:local": "echo 'Access via https://aml-local.janovix.workers.dev (see LOCAL_DEV_SETUP.md)' && next dev --webpack",
		"caddy:local": "caddy run --config Caddyfile",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint . --max-warnings=0",
		"lint:fix": "eslint . --fix",
		"test": "vitest run --coverage",
		"test:unit": "vitest run",
		"test:watch": "vitest",
		"test:mutation": "stryker run",
		"ci:check": "pnpm run format:check && pnpm run lint && pnpm run typecheck && pnpm run test",
		"predeploy:preview": "pnpm run cf-build",
		"versions:upload": "node ./scripts/versions-upload.mjs",
		"storybook": "pnpm exec storybook dev -p 6006",
		"build-storybook": "node scripts/build-storybook.mjs",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"release": "semantic-release",
		"start": "next start"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,md,mdx,json,yml,yaml,css,scss}": [
			"prettier --write",
			"eslint --fix"
		]
	}
